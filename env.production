# Production environment variables for Admin Dashboard

# API Configuration (REQUIRED)
# Set this to your production backend API base URL
VITE_API_BASE_URL=https://api.curk.in/api/v1

NODE_ENV=production

# Authentication Configuration
# Set to 'true' to use API key authentication instead of JWT tokens
VITE_USE_API_KEY=false

# API Key for authentication (only used when VITE_USE_API_KEY=true)
# This should be a secure API key provided by your backend
# VITE_API_KEY=your-production-api-key-here
